#assetList
  li
    display: block
    position: relative
    float: left
    width: 265px
    height: 280px
    margin: 0 10px 10px 0
    padding: 5px;
    background-color: #eee
    .titlelabel
      position: absolute
      top: 0
      left: 0
      padding: 5px
      background-color: #eee
      width: 100%
      opacity: 0.6
      font-weight: bold
      .title
        color: #000
    .imagecontainer
      width: 265px
      line-height: 238px
      text-align: center
      display: inline-block
      background: #444
      height: 240px
      margin: -5px -5px 0 -5px
      .icon
        font-size: 90px
        i
          color: #f6f6f6
      img
        max-width: 265px
        max-height: 240px
    .controls
      line-height: 30px
      width: 100%
      height: 35px
      display: inline-block
      padding-top: 5px
      a
        float: right
        margin-left: 5px
